# InvoiceGen Docker Environment Configuration  
# Copy this file to .env before running docker-compose  
  
APP_PORT=3000  
NODE_ENV=production  
  
POSTGRES_USER=invoicegen  
POSTGRES_PASSWORD=your_secure_password_here  
POSTGRES_DB=invoicegen  
POSTGRES_PORT=5432 
  
DATABASE_URL=postgresql://invoicegen:your_secure_password_here@postgres:5432/invoicegen  
  
BETTER_AUTH_SECRET=your_random_secret_key_minimum_32_characters  
BETTER_AUTH_URL=http://localhost:3000  
  
CLOUDFLARE_R2_ACCOUNT_ID=your_account_id  
CLOUDFLARE_R2_ACCESS_KEY_ID=your_access_key  
CLOUDFLARE_R2_SECRET_ACCESS_KEY=your_secret_key  
CLOUDFLARE_R2_BUCKET_NAME=invoicegen-storage  
CLOUDFLARE_R2_ENDPOINT=https://your_account_id.r2.cloudflarestorage.com 
  
GEMINI_API_KEY=  
  
GMAIL_EMAIL=  
GMAIL_APP_PASSWORD= 
